#include <stdio.h>




int main() {
    int n;
    double v1, v2, v3, media;

    scanf("%d", &n);

    for(int i=0; i<n; i++){
            media = 0;
            scanf("%lf %lf %lf", &v1, &v2, &v3);

            media=((2*v1)+(3*v2)+(5*v3))/10;
        printf("%.1lf\n", media);

    }


    return 0;
}
